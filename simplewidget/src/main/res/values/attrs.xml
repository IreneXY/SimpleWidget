<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SimpleProgressBar">
        <attr name="spb_background_color" format="color" />
        <attr name="spb_foreground_color" format="color" />
        <attr name="spb_progress" format="float" />
        <attr name="spb_corner" format="dimension" />
        <attr name="spb_shadow_color" format="color"/>
        <attr name="spb_shadow_border_width" format="dimension"/>
        <attr name="spb_shadow_corner" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SimpleIconButton">
        <attr name="sib_background_color" format="color" />
        <attr name="sib_text" format="reference"/>
        <attr name="sib_text_color" format="reference" />
        <attr name="sib_text_size" format="dimension" />
        <attr name="sib_corner" format="dimension" />
        <attr name="sib_icon" format="reference"/>
        <attr name="sib_icon_size" format="dimension"/>
        <attr name="sib_icon_color" format="reference" />

    </declare-styleable>

</resources>